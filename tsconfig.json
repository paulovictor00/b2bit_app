{
"compilerOptions": {
  "target": "ES2020",
  "lib": ["ES2020", "DOM", "DOM.Iterable"],
  "module": "ESNext",
  "jsx": "react-jsx",
  "moduleResolution": "Bundler",
  "strict": true,
  "resolveJsonModule": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "noEmit": true,
  "types": ["vite/client", "jest", "node"]
},
"include": ["src", "jest.setup.ts"]
}
